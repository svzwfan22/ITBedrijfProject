@model ITBedrijfProject.Models.Register


@{
    ViewBag.Title = "NewRegister";
}

<h2>NewRegister</h2>

<h2>New Organisation</h2>
@using (Html.BeginForm("NewRegister", "Kassa", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <table>
        <tr>
            <td><label>RegisterName</label></td>
            <td><input type="text" name="RegisterName" /></td>
        </tr>
        <tr>
            <td><label>Device</label></td>
            <td><input type="text" name="Device" /></td>
        </tr>
        <tr>
            <td><label>PurchaseDate</label></td>
            <td><input type="date" name="PurchaseDate" /></td>
        </tr>
        <tr>
            <td><label>ExpiresDate</label></td>
            <td><input type="date" name="ExpiresDate" /></td>
        </tr>
       
    </table>
    <br />

    <input type="submit" value="Create" />
}
